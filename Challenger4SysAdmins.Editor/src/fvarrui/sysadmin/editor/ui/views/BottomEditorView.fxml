<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="view" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Separator halignment="LEFT" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
      <FlowPane alignment="CENTER_RIGHT" hgap="5.0" HBox.hgrow="ALWAYS">
         <children>
            <Button defaultButton="true" mnemonicParsing="false" text="Create Challenge">
               <tooltip>
                  <Tooltip text="Create Challenge" />
               </tooltip></Button>
            <Button mnemonicParsing="false" text="Edit Challenge">
               <tooltip>
                  <Tooltip text="Edit Challenge " />
               </tooltip></Button>
            <Button cancelButton="true" mnemonicParsing="false" text="Exit">
               <tooltip>
                  <Tooltip text="Exit Aplication" />
               </tooltip></Button>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </FlowPane>
   </children>
</HBox>
