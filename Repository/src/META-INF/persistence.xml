<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">

	<persistence-unit name="challenge">
	
		<provider>com.objectdb.jpa.Provider</provider>
		
		<class>fvarrui.sysadmin.challenger.repository.entities.CommandEntity</class>
		<class>fvarrui.sysadmin.challenger.repository.entities.ShellEntity</class>
		
		<properties>
			<property name="javax.persistence.jdbc.url" value="repo/challenge.odb;drop" />
			<property name="javax.persistence.jdbc.user" value="" />
			<property name="javax.persistence.jdbc.password" value="" />
		</properties>
		
	</persistence-unit>

</persistence>